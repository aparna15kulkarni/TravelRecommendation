			<!-- Main -->
				<section id="main" class="container 75%">
					<header>
						<h2>Signup</h2>
						<p>Sign Up to connect with you.</p>
					</header>
					<div class="box">
						<form method="post" action="Signup1"  ng-app="LocationRecommendation" ng-controller="SignupCtrl">
							<div class="row uniform 50%">
<div class="12u">
								<div class="12u">
<input type="firstName" ng-model="firstname" name="firstName" id="firstName" value="" placeholder="First Name" />
									
								</div>


	<div class="12u">
<input type="lastName" ng-model="lastname" name="lastName" id="lastName" value="" placeholder="Last Name" />
									
								</div>

	<div class="12u">
		<input type="email" ng-model="email" name="email" id="email" value="" placeholder="Email" />
		</div>
							</div>
	<div class="row uniform 50%">
		<div class="12u">
		<input type="text" ng-model="password" name="password" id="password" value="" placeholder="Password" />
		</div>
	<div class="12u">
	
  <input type="text" name="confirmPassword" id="confirmPassword" value="" placeholder="Confirm password" />
	</div>

							</div>
							<div class="row uniform 50%">
															</div>
							<div class="row uniform">
								<div class="12u">
									<ul class="actions align-center">
										<li><input type="submit" value="Sign Up" /></li>
									</ul>
								</div>
								<p><%= typeof Message!='undefined' ? Message: '' %></p>
							</div>
						</form>
					</div>
				</section>

